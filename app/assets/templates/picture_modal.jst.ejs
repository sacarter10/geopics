<div class="modal fade" id="picture-modal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Pictures from <%= venue.name %></h4>
      </div>
      <div class="modal-body">
        <div id="carousel-example-generic" class="carousel slide">
  <div class="carousel-inner" id="picture-carousel">
    <% _.each(venue.pictures.images, function (picture) { %>
    <div class="item active">
      <img src="<%= picture.standard_resolution.url %>" alt="bunny">
    </div>
    <% }) %>
  </div>

  <a class="left carousel-control" href="#picture-carousel" data-slide="prev">
    <span class="icon-prev"></span>
  </a>
  <a class="right carousel-control" href="#picture-carousel" data-slide="next">
    <span class="icon-next"></span>
  </a>
</div>
      </div>
    </div>
  </div>
</div>