<h4><a href="<%= venue.canonicalUrl %>"><%= venue.name %></a></h4>
<%= venue.location.address %><br>
<%= venue.location.city %>, <%= venue.location.state %>, <%= venue.location.country %><br>

<% if (venue.pictures.length > 0) { %>
	<% _.each(venue.pictures, function(picture) { %>
		<img src="<%= picture.images.thumbnail.url %>" id="thumbnail">
	<% }) %>
<% } else { %>
	No images found for this location.
<% } %>

<br>
<button type="submit" class="btn btn-default btn-medium" id="feed-button">
	Feed
</button>

<div class="modal fade" id="picture-modal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Pictures from <%= venue.name %></h4>
      </div>
      <div class="modal-body">
        <div id="picture-carousel" class="carousel slide">
  <div class="carousel-inner">
    <% _.each(venue.pictures, function (picture) { %>
    <div class="item">
      <img src="<%= picture.images.low_resolution.url %>" style="margin-left: auto; margin-right: auto">
    </div>
    <% }) %>
  </div>

  <a class="left carousel-control" href="#picture-carousel" data-slide="prev">
    <span class="icon-prev"></span>
  </a>
  <a class="right carousel-control" href="#picture-carousel" data-slide="next">
    <span class="icon-next"></span>
  </a>
</div>
      </div>
    </div>
  </div>
</div>